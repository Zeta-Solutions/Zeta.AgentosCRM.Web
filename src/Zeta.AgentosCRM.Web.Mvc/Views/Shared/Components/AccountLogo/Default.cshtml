@using Abp.Timing
@model Zeta.AgentosCRM.Web.Views.Shared.Components.AccountLogo.AccountLogoViewModel
@{
    var theme = await GetTheme();
}
<a href="~/" class="d-flex justify-content-center mb-6 mb-6">
    <img src="@Model.GetLogoUrl(ApplicationPath, theme.BaseSettings.Layout.DarkMode ? "dark" : "light")&t=@Clock.Now.Ticks.ToString()" alt="" class="h-80px"/>
</a>
