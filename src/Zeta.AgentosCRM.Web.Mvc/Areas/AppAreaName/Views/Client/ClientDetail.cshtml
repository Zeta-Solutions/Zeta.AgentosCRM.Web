        <div class="card-Head">
                <div class="d-flex h-55px navbar-link-card-Allign">
                <ul class="
         nav nav-stretch nav-line-tabs nav-line-tabs-2x
          border-transparent
          fs-5
          fw-bolder

          ml-3 pl-2
        ">
                    <li class="nav-item">
                        <a href="#ActivitiesTab" data-bs-toggle="tab" role="tab" class="nav-link navbar-link-item-card-font ">
                            @L("Activities")
                        </a>
                    </li>
                    <li class="nav-item">
                            <a href="#ApplicationTab" data-bs-toggle="tab" role="tab" class="nav-link navbar-link-item-card-font active" aria-selected="true">
                            @L("Application")
