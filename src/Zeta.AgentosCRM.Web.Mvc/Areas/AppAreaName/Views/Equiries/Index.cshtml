@using Zeta.AgentosCRM.Authorization
@using Zeta.AgentosCRM.Web.Areas.AppAreaName.Models.Clients
@using Zeta.AgentosCRM.Web.Areas.AppAreaName.Startup
@model ClientsViewModel
@{
    ViewBag.CurrentPageName = AppAreaNamePageNames.Tenant.Equiries;
}
@section Styles
	{
	<link rel="stylesheet" abp-href="/view-resources/Areas/AppAreaName/Views/Common/_PartialCardView.css" asp-append-version="true" />
	 <link rel="stylesheet" abp-href="/view-resources/Areas/AppAreaName/Views/_Bundles/demo-ui-components.css" asp-append-version="true" />
   }
@section Scripts
{
	<script abp-src="/view-resources/Areas/AppAreaName/Views/_Bundles/demo-ui-components.js" asp-append-version="true"></script>
}



                <abp-page-subheader title='@L("Equiry")' description='@L("EquiriesHeaderInfo")'>
    
     <button id="ExportToExcelButton" hidden class="btn btn-white btn-outline btn-sm  me-1 rounded-0"><i class="fa fa-file-excel text-success"></i> @L("ExportToExcel")</button>
                </abp-page-subheader>
            
<div class="@(await GetContainerClass())">   
    <div class="card card-custom gutter-b">
        <div class="card-body">
            <div class="form">
                <div class="row align-items-center mb-4">
                    <div class="col-xl-12">                        
                        
                        
                <div class="my-3">
                    <div class="input-group">
                        <input type="text" id="ClientsTableFilter" class="form-control form-control-sm  reload-on-change" placeholder='@L("SearchWithThreeDot")' value="">
                        <button id="GetClientsButton" class="btn btn-sm rounded-0 btnsearch" type="submit"><i class="flaticon-search-1"></i></button>                        
                        <input type="hidden" id="GetEmail" class="form-control form-control-sm">            
                    </div>
                </div>
            
                    </div>
                </div>
			</div>
                <div id="AdvacedAuditFiltersArea" style="display: none" class="row mb-4">
                    <div class="col-md-12">
						
					</div>


							<div class="col-md-3" hidden >
								<div class="my-3">
                        <label class="form-label fw-bold text-dark" for="TabValues">@L("TabValues")</label>
									<input type="text" class="form-control custom-small-input rounded-0 input-sm reload-on-keyup" name="TabValues" id="TabValues" value="0">
								</div>
                            </div>
							<div class="col-md-3">
								<div class="my-3">
									<label class="form-label fw-bold text-dark" for="FirstNameFilterId">@L("FirstName")</label>
									<input type="text" class="form-control custom-small-input rounded-0 input-sm reload-on-keyup" name="firstNameFilter" id="FirstNameFilterId">
								</div>
                            </div>
							<div class="col-md-3">
								<div class="my-3">
									<label class="form-label fw-bold text-dark" for="LastNameFilterId">@L("LastName")</label>
									<input type="text" class="form-control custom-small-input rounded-0 input-sm reload-on-keyup" name="lastNameFilter" id="LastNameFilterId">
								</div>
                            </div>
							<div class="col-md-3" hidden>
								<div class="my-3">
									<label class="form-label fw-bold text-dark" for="EmailFilterId">@L("Email")</label>
									<input type="text" class="form-control custom-small-input rounded-0 input-sm reload-on-keyup" name="emailFilter" id="EmailFilterId">
								</div>
                            </div>
							<div class="col-md-3">
								<div class="my-3">
									<label class="form-label fw-bold text-dark" for="PhoneNoFilterId">@L("PhoneNo")</label>
									<input type="text" class="form-control custom-small-input rounded-0 input-sm reload-on-keyup" name="phoneNoFilter" id="PhoneNoFilterId">
								</div>
                            </div>
                            <div class="col-md-3" hidden>
                                    <div class="my-3">
										<label class="form-label fw-bold text-dark" for="MinDateofBirthFilterId">@L("DateofBirthRange")</label>
										<div class="input-group">
											<input class="form-control custom-small-input rounded-0 input-sm reload-on-keyup m-input date-picker startDate" type="text" placeholder="@L("MinValue")"  id="MinDateofBirthFilterId">
											<span style="line-height: 40px;padding: 0 15px 0 15px;"> &mdash; </span>
							<input class="form-control custom-small-input rounded-0 input-sm reload-on-keyup m-input date-picker endDate" type="text" placeholder="@L("MaxValue")" id="MaxDateofBirthFilterId">
										</div>
									</div>
                            </div>
							<div class="col-md-3" hidden>
								<div class="my-3">
									<label class="form-label fw-bold text-dark" for="UniversityFilterId">@L("University")</label>
									<input type="text" class="form-control custom-small-input rounded-0 input-sm reload-on-keyup" name="universityFilter" id="UniversityFilterId">
								</div>
                            </div>
                            <div class="col-md-3" hidden>
                                 <div class="my-3">
									<label class="form-label fw-bold text-dark" for="MinRatingFilterId">(@L("MinMax")) @L("Rating")</label>
									<div class="input-group">
										<input type="number" class="form-control custom-small-input rounded-0 input-sm reload-on-keyup" placeholder="@L("MinValue")" name="minRatingFilter" id="MinRatingFilterId"/>
                                        <input type="number" class="form-control custom-small-input rounded-0 input-sm reload-on-keyup" placeholder="@L("MaxValue")" name="maxRatingFilter"  id="MaxRatingFilterId"/>
									</div>
                                </div>
                            </div>


							<div class="col-md-3" hidden>
								<div class="my-3">
									<label class="form-label fw-bold text-dark" for="CountryDisplayPropertyFilterId">(@L("Country")) @L("DisplayProperty")</label>
									<input type="text" class="form-control custom-small-input rounded-0 input-sm reload-on-keyup" name="countryDisplayPropertyFilter" id="CountryDisplayPropertyFilterId">
								</div>
                            </div>
							<div class="col-md-3">
								<div class="my-3">
									<label class="form-label fw-bold text-dark" for="UserNameFilterId">(@L("User")) @L("Name")</label>
									<input type="text" class="form-control custom-small-input rounded-0 input-sm reload-on-keyup" name="userNameFilter" id="UserNameFilterId">
								</div>
                            </div>
							<div class="col-md-3" hidden>
								<div class="my-3">
									<label class="form-label fw-bold text-dark" for="BinaryObjectDescriptionFilterId">(@L("BinaryObject")) @L("Description")</label>
									<input type="text" class="form-control custom-small-input rounded-0 input-sm reload-on-keyup" name="binaryObjectDescriptionFilter" id="BinaryObjectDescriptionFilterId">
								</div>
                            </div>
							<div class="col-md-3" hidden>
								<div class="my-3">
									<label class="form-label fw-bold text-dark" for="DegreeLevelNameFilterId">(@L("DegreeLevel")) @L("Name")</label>
									<input type="text" class="form-control custom-small-input rounded-0 input-sm reload-on-keyup" name="degreeLevelNameFilter" id="DegreeLevelNameFilterId">
								</div>
                            </div>
							<div class="col-md-3" hidden>
								<div class="my-3">
									<label class="form-label fw-bold text-dark" for="SubjectAreaNameFilterId">(@L("SubjectArea")) @L("Name")</label>
									<input type="text" class="form-control custom-small-input rounded-0 input-sm reload-on-keyup" name="subjectAreaNameFilter" id="SubjectAreaNameFilterId">
								</div>
                            </div>
							<div class="col-md-3" hidden>
								<div class="my-3">
									<label class="form-label fw-bold text-dark" for="LeadSourceNameFilterId">(@L("LeadSource")) @L("Name")</label>
									<input type="text" class="form-control custom-small-input rounded-0 input-sm reload-on-keyup" name="leadSourceNameFilter" id="LeadSourceNameFilterId">
								</div>
                            </div>

							<div class="col-md-3">
								<div class="my-3">
									<label class="form-label fw-bold text-dark" for="CountryName3FilterId">(@L("Country")) @L("Name")</label>
						<input type="text" class="form-control custom-small-input rounded-0 input-sm reload-on-keyup" name="countryName3Filter" id="CountryName3FilterId">
								</div>
                            </div>
                    <div class="col-md-12 mt-5">
                        <button class="btn btn-secondary btn-sm" id="btn-reset-filters">Reset</button>
                    </div>
                </div>
                <div class="row my-4">
                    <div class="col-xl-12">
                        <span id="ShowAdvancedFiltersSpan" class="text-muted clickable-item"><i class="fa fa-angle-down"></i> @L("ShowAdvancedFilters")</span>
                        <span id="HideAdvancedFiltersSpan" class="text-muted clickable-item" style="display: none"><i class="fa fa-angle-up"></i> @L("HideAdvancedFilters")</span>
                    </div>
                </div> 

        <div class="card navbar-link-card-Tab-height pt-1 pr-1 pl-2 d-flex">
                    <div class="card-Head CardTab">
                <div class="d-flex h-30px navbar-link-card-Allign">
                <ul class="nav nav-stretch nav-line-tabs nav-line-tabs-2x border-transparent fs-5 fw-bolder ml-3 pl-2" id="tabs">
                    <li class="nav-item">
                            <a href="#ApplicationTab" id="clients" data-bs-toggle="tab" role="tab" class="nav-link navbar-link-item-card-font " >
									@L("Equiries")
                        </a>
                    </li>
                    
                    <li class="nav-item">
                            <a href="#ApplicationTab" id="Archived" data-bs-toggle="tab" role="tab" class="nav-link navbar-link-item-card-font " >
                            @L("Archived")
                        </a>
                    </li>
                    
                </ul>
            </div>
        </div>
    </div>
            <div class="row align-items-center">
                <table id="ClientsTable" class="table align-middle  fs-8 gy-2 dataTable no-footer">
                    <thead class="thead gy-7">
                        <tr>
							   <th class="align-middle" style="padding:2px"></th>
                                <th><input id="SelectAllCheckBox" type="checkbox"></th>
								<th >@L("Name")</th>
								<th >@L("AddedFrom")</th>
								<th >@L("Tagname")</th>
								<th >@L("Phone")</th>
								<th>@L("currentcity")</th>
								<th>@L("VisaExpirayDate")</th>
								<th>@L("AddedOn")</th>
							     <th>@L("LastUpdate")</th>
								<th>@L("PreferredIntake")</th>
								<th >@L("Pref.Office")</th>
							     <th>@L("InQueue")</th>
							     <th>@L("Pref.Service")</th>
								
								<th></th>
                                                 
                        </tr>
                    </thead>
                </table>
            </div>
        </div>
    </div>
  </div>
