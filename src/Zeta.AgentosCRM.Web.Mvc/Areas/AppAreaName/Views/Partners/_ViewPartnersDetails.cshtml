@using Zeta.AgentosCRM.Authorization
@using Zeta.AgentosCRM.Web.Areas.AppAreaName.Startup;
@{
    ViewBag.CurrentPageName = AppAreaNamePageNames.Tenant.DegreeLevel;
}

@section Styles
    {
    <link rel="stylesheet" abp-href="/view-resources/Areas/AppAreaName/Views/Common/_PartialCardView.css" asp-append-version="true" />
  }



<div class="">
    <abp-page-subheader title='@L("MainForm")' description='@L("MainFormInfo")'>


    </abp-page-subheader>
    <div class="card maincard">

        <div class="card-body">
            <div class="d-flex flex-wrap flex-sm-nowrap mb-3">
                <div class="me-7 mb-4">
                    <div class="
            symbol symbol-100px symbol-lg-160px symbol-fixed
            position-relative
          ">
                        <img src="/Profile/GetProfilePicture" alt="Metronic" />
                        <div class="
              position-absolute
              translate-middle
              bottom-0
              start-100
              mb-6
              bg-success
              rounded-circle
              border border-4 border-white
              h-20px
              w-20px
            "></div>
                    </div>
                </div>

                <div class="flex-grow-1">
                    <div class="
            d-flex
            justify-content-between
            align-items-start
            flex-wrap
            mb-2
          ">
                        <div class="d-flex flex-column">
                            <div class="d-flex align-items-center mb-2">
                                <a class="
                  text-gray-800 text-hover-primary
                  fs-2
                  fw-bolder
                  me-1
                  cursor-pointer
                ">
                                    Max Smith
                                </a>
                                <a class="cursor-pointer">
                                    <span [inlineSVG]="
                    './assets/media/icons/duotune/general/gen026.svg'
                  "
                                          class="svg-icon svg-icon-1 svg-icon-primary"></span>
                                </a>
                                <a class="
                  btn btn-sm btn-light-success
                  fw-bolder
                  ms-2
                  fs-8
                  py-1
                  px-3
                  cursor-pointer
                "
                                   data-bs-toggle="modal"
                                   data-bs-target="#kt_modal_upgrade_plan">
                                    Upgrade to Pro
                                </a>
                            </div>

                            <div class="d-flex flex-wrap fw-bold fs-6 mb-4 pe-2">
                                <a class="
                  d-flex
                  align-items-center
                  text-gray-400 text-hover-primary
                  me-5
                  mb-2
                  cursor-pointer
                ">
                                    <span [inlineSVG]="
                    './assets/media/icons/duotune/communication/com006.svg'
                  "
                                          class="svg-icon svg-icon-4 me-1"></span>
                                    Developer
                                </a>
                                <a class="
                  d-flex
                  align-items-center
                  text-gray-400 text-hover-primary
                  me-5
                  mb-2
                  cursor-pointer
                ">
                                    <span [inlineSVG]="
                    './assets/media/icons/duotune/general/gen018.svg'
                  "
                                          class="svg-icon svg-icon-4 me-1"></span>
                                    SF, Bay Area
                                </a>
                                <a class="
                  d-flex
                  align-items-center
                  text-gray-400 text-hover-primary
                  mb-2
                  cursor-pointer
                ">
                                    <span [inlineSVG]="
                    './assets/media/icons/duotune/communication/com011.svg'
                  "
                                          class="svg-icon svg-icon-4 me-1"></span>
                                    mailto:max@kt.com
                                </a>
                            </div>
                        </div>

                        <div class="d-flex my-4">
                            <a class="btn btn-sm btn-light me-2 cursor-pointer"
                               id="kt_user_follow_button">
                                <span [inlineSVG]="'./assets/media/icons/duotune/arrows/arr012.svg'"
                                      class="svg-icon svg-icon-3 d-none"></span>
                                <span class="indicator-label">Follow</span>
                                <span class="indicator-progress">
                                    Please wait...
                                    <span class="spinner-border spinner-border-sm align-middle ms-2"></span>
                                </span>
                            </a>
                            <a class="btn btn-sm btn-primary me-3 cursor-pointer"
                               data-bs-toggle="modal"
                               data-bs-target="#kt_modal_offer_a_deal">
                                Hire Me
                            </a>
                            <div class="me-0">
                                <button class="
                  btn btn-sm btn-icon btn-bg-light btn-active-color-primary
                "
                                        data-kt-menu-trigger="click"
                                        data-kt-menu-placement="bottom-end"
                                        data-kt-menu-flip="top-end">
                                    <i class="bi bi-three-dots fs-3"></i>
                                </button>
                                <app-dropdown-menu1></app-dropdown-menu1>
                            </div>
                        </div>
                    </div>

                    <div class="d-flex flex-wrap flex-stack">
                        <div class="d-flex flex-column flex-grow-1 pe-8">
                            <div class="d-flex flex-wrap">
                                <div class="
                  border border-gray-300 border-dashed
                  rounded
                  min-w-125px
                  py-3
                  px-4
                  me-6
                  mb-3
                ">
                                    <div class="d-flex align-items-center">
                                        <span [inlineSVG]="
                      './assets/media/icons/duotune/arrows/arr066.svg'
                    "
                                              class="svg-icon svg-icon-3 svg-icon-success me-2"></span>
                                        <div class="fs-2 fw-bolder">4500$</div>
                                    </div>

                                    <div class="fw-bold fs-6 text-gray-400">Earnings</div>
                                </div>

                                <div class="
                  border border-gray-300 border-dashed
                  rounded
                  min-w-125px
                  py-3
                  px-4
                  me-6
                  mb-3
                ">
                                    <div class="d-flex align-items-center">
                                        <span [inlineSVG]="
                      './assets/media/icons/duotune/arrows/arr065.svg'
                    "
                                              class="svg-icon svg-icon-3 svg-icon-danger me-2"></span>
                                        <div class="fs-2 fw-bolder">75</div>
                                    </div>

                                    <div class="fw-bold fs-6 text-gray-400">Projects</div>
                                </div>

                                <div class="
                  border border-gray-300 border-dashed
                  rounded
                  min-w-125px
                  py-3
                  px-4
                  me-6
                  mb-3
                ">
                                    <div class="d-flex align-items-center">
                                        <span [inlineSVG]="
                      './assets/media/icons/duotune/arrows/arr066.svg'
                    "
                                              class="svg-icon svg-icon-3 svg-icon-success me-2"></span>
                                        <div class="fs-2 fw-bolder">60%</div>
                                    </div>

                                    <div class="fw-bold fs-6 text-gray-400">Success Rate</div>
                                </div>
                            </div>
                        </div>

                        <div class="
              d-flex
              align-items-center
              w-200px w-sm-300px
              flex-column
              mt-3
            ">
                            <div class="d-flex justify-content-between w-100 mt-auto mb-2">
                                <span class="fw-bold fs-6 text-gray-400">Profile Compleation</span>
                                <span class="fw-bolder fs-6">50%</span>
                            </div>
                            <div class="h-5px mx-3 w-100 bg-light mb-3">
                                <div class="bg-success rounded h-5px"
                                     role="progressbar"
                                     [style.width]="'50%'"></div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>



        </div>

    </div>
</div>

<div class="mt-2 maincard">
    <div class="card navbar-link-card-height pt-1 pr-1 pb-3 pl-2 d-flex">
        <div class="card-head">
            <div class="d-flex  h-55px navbar-link-card-Allign">
                <ul class="
          nav nav-stretch nav-line-tabs nav-line-tabs-2x
          border-transparent
          fs-5
          fw-bolder

          ml-3 pl-2
        ">
                    <li class="nav-item">
                        <a href="#ApplicationsTab" data-bs-toggle="tab" role="tab" class="nav-link navbar-link-item-card-font">
                            @L("Application")
                        </a>
                    </li>

                    <li class="nav-item">
                        <a href="#ProductsTab" data-bs-toggle="tab" role="tab" class="nav-link navbar-link-item-card-font">
                            @L("Product")
                        </a>
                    </li>
                    <li class="nav-item">
                        <a href="#BranchesTab" data-bs-toggle="tab" role="tab" class="nav-link navbar-link-item-card-font">
                            @L("Branches")
                        </a>
                    </li>
                    <li class="nav-item">
                        <a href="#AgreementsTab" data-bs-toggle="tab" role="tab" class="nav-link navbar-link-item-card-font">
                            @L("Agreements")
                        </a>
                    </li>
                    <li class="nav-item">
                        <a href="#ContactsTab" data-bs-toggle="tab" role="tab" class="nav-link navbar-link-item-card-font">
                            @L("Contacts")
                        </a>
                    </li>
                    <li class="nav-item">
                        <a href="#NotesTermsTab" Tab" data-bs-toggle="tab" role="tab" class="nav-link navbar-link-item-card-font">
                            @L("NotesAndTerms")
                        </a>
                    </li>
                    <li class="nav-item">
                        <a href="#DocumentsTab" data-bs-toggle="tab" role="tab" class="nav-link navbar-link-item-card-font">
                            @L("Documents")
                        </a>
                    </li>
                    <li class="nav-item">
                        <a href="#AppointmentsTab" data-bs-toggle="tab" role="tab" class="nav-link navbar-link-item-card-font">
                            @L("Appointments")
                        </a>
                    </li>
                    <li class="nav-item">
                        <a href="#ConversationsTab" data-bs-toggle="tab" role="tab" class="nav-link navbar-link-item-card-font">
                            @L("Conversations")
                        </a>
                    </li>
                    <li class="nav-item">
                        <a href="#TasksTab" data-bs-toggle="tab" role="tab" class="nav-link navbar-link-item-card-font">
                            @L("CRMTasks")
                        </a>
                    </li>
                      <li class="nav-item">
                        <a href="#OtherInformationTab" data-bs-toggle="tab" role="tab" class="nav-link navbar-link-item-card-font">
                            @L("OtherInformations")
                        </a>
                    </li>
                    <li class="nav-item">
                        <a href="#PromotionsTab" data-bs-toggle="tab" role="tab" class="nav-link navbar-link-item-card-font">
                            @L("Promotions")
                        </a>
                    </li>
                </ul>
            </div>
        </div>
    </div>
</div>

<div>
    <div class="tab-content">
        <div class="tab-pane pt-5" id="ApplicationsTab" role="tabpanel">
            <div class="list-group">
                @await Html.PartialAsync("~/Areas/AppAreaName/Views/Partners/_Applications.cshtml")
            </div>
        </div>

        <div class="tab-pane pt-5 mb-7" id="ProductsTab" role="tabpanel">
            <div class="list-group">
                @await Html.PartialAsync("~/Areas/AppAreaName/Views/Partners/_Products.cshtml")
            </div>
        </div>
        <div class="tab-pane pt-5 mb-7" id="BranchesTab" role="tabpanel">
            <div class="list-group">
                @await Html.PartialAsync("~/Areas/AppAreaName/Views/Partners/_Branches.cshtml")
            </div>
        </div>
        <div class="tab-pane pt-5 mb-7" id="AgreementsTab" role="tabpanel">
            <div class="list-group">
                @await Html.PartialAsync("~/Areas/AppAreaName/Views/Partners/_Agreements.cshtml")
            </div>
        </div>  <div class="tab-pane pt-5 mb-7" id="ContactsTab" role="tabpanel">
            <div class="list-group">
                @await Html.PartialAsync("~/Areas/AppAreaName/Views/Partners/_Contacts.cshtml")
            </div>
        </div>
        <div class="tab-pane pt-5 mb-7" id="NotesTermsTab" role="tabpanel">
            <div class="list-group">
                @await Html.PartialAsync("~/Areas/AppAreaName/Views/Partners/_NotesAndTerms.cshtml")
            </div>
        </div>
        <div class="tab-pane pt-5 mb-7" id="DocumentsTab" role="tabpanel">
            <div class="list-group">
                @await Html.PartialAsync("~/Areas/AppAreaName/Views/Partners/_Documents.cshtml")
            </div>
        </div>
        <div class="tab-pane pt-5 mb-7" id="AppointmentsTab" role="tabpanel">
            <div class="list-group">
                @await Html.PartialAsync("~/Areas/AppAreaName/Views/Partners/_Appointments.cshtml")
            </div>
        </div>
        <div class="tab-pane pt-5 mb-7" id="ConversationsTab" role="tabpanel">
            <div class="list-group">
                @await Html.PartialAsync("~/Areas/AppAreaName/Views/Partners/_Conversation.cshtml")
            </div>
        </div>
        <div class="tab-pane pt-5 mb-7" id="TasksTab" role="tabpanel">
            <div class="list-group">
                @await Html.PartialAsync("~/Areas/AppAreaName/Views/Partners/_Tasks.cshtml")
            </div>
        </div>
        <div class="tab-pane pt-5 mb-7" id="OtherInformationTab" role="tabpanel">
            <div class="list-group">
                @await Html.PartialAsync("~/Areas/AppAreaName/Views/Partners/_OtherInformation.cshtml")
            </div>
        </div>
        <div class="tab-pane pt-5 mb-7" id="PromotionsTab" role="tabpanel">
            <div class="list-group">
                @await Html.PartialAsync("~/Areas/AppAreaName/Views/Partners/_Promotions.cshtml")
            </div>
        </div>

    </div>
</div>

