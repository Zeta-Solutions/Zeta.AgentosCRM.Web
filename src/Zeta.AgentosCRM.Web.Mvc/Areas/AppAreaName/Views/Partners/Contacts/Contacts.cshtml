
@section Scripts
    {
 <script abp-src="/view-resources/Areas/AppAreaName/Views/Partners/Contacts/Contacts.js" asp-append-version="true"></script>
    }
<div class="maincard mt-1">
    <div class="col-md-12 text-end mb-3">

        <button id="CreateNewContactsButton" class="btn  btn-sm btnclose btn-outline text-white rounded-0"><i class="fa fa-plus text-white"></i> @L("AddContacts")</button>
    </div>
    <div class="card">

        <div class="card-body">
            <div class="form">
                <div class="fixed-width-div">
                    <div style="overflow-x: auto;">
                    </div>
                       <div class="row align-items-center">
                <table id="ContactsTable" class="table align-middle  fs-8 gy-2 dataTable no-footer">
                    <thead>
                        <tr>
                            <th></th>
                             
                            <th >@L("Name")</th>
                                    <th >@L("Email")</th>
                                    <th >@L("Department")</th>
                                    <th >@L("Position")</th>
                                    <th >@L("Branch")</th>
                            <th></th>
                        </tr>
                    </thead>
                </table>
            </div>
                </div>
            </div>
        </div>
    </div>
</div>


